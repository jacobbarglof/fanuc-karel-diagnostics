{
	"name": "fanuc-karel-diagnostics",
	"displayName": "Fanuc karel diagnostic messages",
	"description": "Fanuc karel diagnostic messages using ktrans, add ktrans to path before using",
	"version": "0.0.8",
	"publisher": "simonl91",
	"homepage": "https://github.com/simonl91/fanuc-karel-diagnostics/blob/main/README.md",
	"private": false,
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/simonl91/fanuc-karel-diagnostics"
	},
	"engines": {
		"vscode": "^1.73.0"
	},
	"categories": [
		"Programming Languages"
	],
	"activationEvents": [
		"*"
	],
	"contributes": {
		"languages": [
			{
				"id": "fanuc-karel",
				"aliases": [
					"Fanuc-Karel(.kl)",
					"fanuc-karel"
				],
				"extensions": [
					".kl"
				]
			}
		]
	},
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint \"src/**/*.ts\"",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^16.18.34",
		"@types/vscode": "^1.73.0",
		"@typescript-eslint/eslint-plugin": "^5.42.0",
		"@typescript-eslint/parser": "^5.42.0",
		"eslint": "^8.26.0",
		"typescript": "^5.1.3"
	}
}